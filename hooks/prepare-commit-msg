#!/usr/bin/env bash
#
# This hook adds issue/ticket/version number from branch name to every commit message
#

COMMIT_MSG_FILE=$1
COMMIT_SOURCE=$2
SHA1=$3

echo "Commit message file: $COMMIT_MSG_FILE"
echo "Commit source: $COMMIT_SOURCE"
echo "Commit SHA1: $SHA1"
echo '-=-=-=-=-=-'
cat $COMMIT_MSG_FILE
echo '-=-=-=-=-=-'
